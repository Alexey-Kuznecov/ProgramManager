<UserControl
    x:Class="ProgramManager.Views.DialogPacks.IconCanvas"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="clr-namespace:ProgramManager.Converters"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewModels="clr-namespace:ProgramManager.ViewModels"
    d:DataContext="{d:DesignInstance viewModels:IconViewModel}"
    d:DesignHeight="300"
    d:DesignWidth="300"
    mc:Ignorable="d">
    <Grid Width="48" Height="48">
        <Button Background="{Binding IconButton}" Command="{Binding CmdSelectDialogIcon}" />
    </Grid>
</UserControl>
