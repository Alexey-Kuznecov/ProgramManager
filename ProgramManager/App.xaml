    x:Class="ProgramManager.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:ProgramManager"
    xmlns:views="clr-namespace:ProgramManager.Views">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/ProgramManager;component/Resources/Icons.xaml" />
                <ResourceDictionary Source="/ProgramManager;component/Resources/Generic.xaml" />
                <ResourceDictionary Source="/ProgramManager;component/Resources/IconStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
